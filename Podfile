# Uncomment this line to define a global platform for your project
platform :ios, '9.0'
use_frameworks!

target 'MaidMe' do
    
    # Lib for making request
    pod 'Alamofire', '~> 4.6'
    pod 'SwiftyJSON', '~> 4.0'
    
    # Check internet connection
    pod 'ReachabilitySwift', '~> 4.1'
    
    # Validate Phone number
    pod 'PhoneNumberKit', '~> 2.1'
    
    # Encrypt data
    pod 'CryptoSwift', '~>  0.8'
    
    pod 'SSKeychain'
    
    # Custom cell with action
    pod 'SWTableViewCell', '~> 0.3.7'
    
    
    # Google Places search address
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'GoogleMaps'
    pod 'GooglePlacesSearchController'
    
    # Image caching
    pod 'SDWebImage'
    
    # Activity Indicator
    pod 'SVProgressHUD'
    
    # Realm
    pod 'RealmSwift', '~> 3.0'
    
    # SDVersion
    pod 'SDVersion'
    
    # Tooltip view
    pod 'EasyTipView', '~> 1.0'
    
    # Fabric
    pod 'Fabric', '~> 1.6'
    pod 'Crashlytics'
    
end


target 'MaidMeTests' do
    
end

target 'MaidMeUITests' do
    
end

